#include <stdio.h>
#include <math.h>
int main()
{
	int a, b;
	printf("Write two numbers\n");
	scanf_s("%i ", &a);
	scanf_s("%i", &b);
	printf("Chose operation\n");
	printf("1. *\n");
	printf("2. /\n");
	printf("3. +\n");
	printf("4. -\n");
	printf("Your choice\n");
	int input;
	scanf_s("%i", &input);
	switch (input) {
	case 1 :
		printf("%i", a*b );
		break;
	case 2:
		printf("%i", a / b);
		break;
	case 3:
		printf("%i", a + b);
		break;
	case 4:
		printf("%i", a - b);
		break;
	default:
		printf("error 666");

	}
	return 0;
}
